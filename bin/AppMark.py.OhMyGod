    return compareTime(time.groups(),tuple(now_t),0) 

def compareTime(m_time,n_time,i):
    if m_time > n_time:
       return False
    if m_time < n_time:
       return True
    if i == len(m_time)-1:
       return True
    else:
       return compareTime(m_time,n_time,i+1)         

